        <script>
            function error() {
                Swal.fire(
                    'Gagal',
                    'Email atau kata sandi tidak sesuai.',
                    'error'
                );
            }

            function berhasilAdmin() {
                Swal.fire(
                    'Berhasil',
                    'Berhasil login, Anda adalah Admin.',
                    'success'
                ).then(function () {
                    window.location = '/study-tracer/Pages/Admin/Dashboard.aspx';
                });
            }   

            function berhasilAlumni() {
                Swal.fire(
                    'Berhasil',
                    'Berhasil login, Anda adalah Alumni.',
                    'success'
                ).then(function () {
                    window.location = '/study-tracer/Pages/Alumni/Dashboard.aspx';
                });
            }

            function berhasilDaftar() {
                Swal.fire(
                    'Berhasil',
                    'Akun berhasil didaftarkan, silahkan buka email Anda untuk verifikasi akun Tracer Study.',
                    'success'
                ).then(function () {
                    window.location = '/study-tracer';
                });
            }

            function berhasilVerifikasiEmail() {
                Swal.fire(
                    'Berhasil',
                    'Email berhasil diverifikasi, silahkan login untuk mengakses Tracer Study.',
                    'success'
                ).then(function () {
                    window.location = '/study-tracer';
                });
            }

            function belumDiverifikasi() {
                Swal.fire(
                    'Peringatan',
                    'Akun Anda belum diverifikasi.',
                    'warning'
                );
            }

            function ditolak() {
                Swal.fire(
                    'Gagal',
                    'Akun Anda telah ditolak, tidak bisa login.',
                    'error'
                );
            }

            function dataRAsama() {
                Swal.fire(
                    'Gagal',
                    'Email Anda sudah pernah didaftarkan sebelumnya.',
                    'error'
                );
            }
        </script>

